{
  "name": "gatsby-starter",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-starter",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build:clean": "gatsby clean && gatsby build --log-pages",
    "build": "gatsby build --log-pages",
    "build:functions": "netlify-lambda build src/functions",
    "start:functions": "netlify-lambda serve src/functions",
    "start:verbose": "gatsby develop --verbose",
    "start:clean": "gatsby clean && gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "bump": "yarn upgrade-interactive --latest",
    "purge": "rm -rf node_modules && yarn",
    "deploy:gh": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:production:ntf": "gatsby build && netlify deploy --prod",
    "deploy:preview:ntf": "gatsby build && netlify deploy",
    "start:concurrently": "concurrently \"yarn start:clean\" \"yarn start:functions \"",
    "build:concurrently": "concurrently \"yarn build:clean\" \"yarn build:functions \""
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-mdx": "^1.10.0",
    "gatsby-plugin-netlify-cms": "^4.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "netlify-cms-app": "^2.14.18",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "gh-pages": "^3.1.0",
    "http-proxy-middleware": "^1.0.6",
    "netlify-lambda": "^2.0.3"
  }
}
